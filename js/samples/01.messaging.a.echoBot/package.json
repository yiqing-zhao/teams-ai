{
    "name": "echobot-demo",
    "version": "1.0.0",
    "description": "A simple echo bot sample",
    "author": "Microsoft",
    "license": "MIT",
    "main": "./lib/index.js",
    "scripts": {
        "build": "tsc --build",
        "clean": "rimraf node_modules lib tsconfig.tsbuildinfo",
        "lint": "eslint **/**/src/**/*.{j,t}s{,x} --fix --no-error-on-unmatched-pattern",
        "start": "cross-env DEBUG=echo* tsc --build && node -r dotenv/config .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "cross-env DEBUG=echo* nodemon --watch ./src -e ts --exec \"yarn start\"",
        "dev:teamsfx": "cross-env DEBUG=echo* nodemon --exec node --inspect=9239 --signal SIGINT -r ts-node/register -r dotenv/config ./src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@microsoft/teams-ai": "~1.0.1",
        "botbuilder": "^4.21.4",
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "@types/dotenv": "8.2.0",
        "@types/express": "^4.17.21",
        "cross-env": "^7.0.3",
        "nodemon": "~3.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
